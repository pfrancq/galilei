<!DOCTYPE kpartgui>
<kpartgui name="kgalileicenter" version="0.1">
<MenuBar>
	<Menu name="connect"><text>&amp;Session</text>
		<Action name="sessionAlwaysCalc"/>
		<Separator/>
		<Action name="sessionConnect"/>
		<Action name="sessionCompute"/>
		<Action name="sessionDisconnect"/>
		<separator/>
		<Action name="sessionTest"/>
		<separator/>
		<Action name="sessionQuit"/>
	</Menu>
	<Menu name="Users"><text>&amp;Users</text>
		<Action name="profileAlwaysCalc"/>
		<Separator/>
		<Action name="showUsers"/>
		<Action name="addUser"/>
		<Action name="addProfile"/>
		<Action name="profilesCalc"/>
		<Separator/>
		<Action name="profileCalc"/>
	</Menu>
	<Menu name="Groups"><text>&amp;Groups</text>
		<Action name="groupAlwaysCalc"/>
		<Separator/>
		<Action name="showGroups"/>
		<Action name="groupsCalc"/>
		<Action name="groupingCompare"/>
	</Menu>
	<Menu name="Documents"><text>&amp;Documents</text>
		<Action name="docAlwaysCalc"/>
		<Separator/>
		<Action name="showDocs"/>
		<Action name="addDoc"/>
		<Action name="docsAnalyse"/>
		<Action name="docsStats"/>
		<separator/>
		<Action name="addJudgement"/>
		<Action name="editJudgement"/>
		<Action name="createXML"/>
		<Action name="analyseXML"/>
	</Menu>
	<Menu name="Text"><text>&amp;Text</text>
		<Separator/>
		<Action name="textFrench"/>
		<Action name="textEnglish"/>
	</Menu>
	<Menu name="GA"><text>&amp;GA</text>
		<Action name="gaInit"/>
		<Separator/>
		<Action name="gaStart"/>
		<Action name="gaPause"/>
		<Action name="gaStop"/>
	</Menu>
	<Menu name="settings"><text>&amp;Settings</text>
		<Action name="plugins"/>
		<Action name="docsOptions"/>
	</Menu>
	<Menu name="program"><text>&amp;Program</text>
		<Action name="loadProgram"/>
	</Menu>
	<Action name="window_menu"/>
</MenuBar>
<ToolBar fullWidth="true" name="mainToolBar">
	<Action name="sessionConnect"/>
	<Separator/>
	<Action name="showUsers"/>
	<Action name="profilecalc"/>
	<Separator/>
	<Action name="showGroups"/>
	<Action name="groupsCalc"/>
	<Separator/>
	<Action name="showDocs"/>
	<Action name="createXML"/>
	<Action name="analyseXML"/>
	<Separator/>
	<Action name="sessionQuit"/>
</ToolBar>
</kpartgui>
